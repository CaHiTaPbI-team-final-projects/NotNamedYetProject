<Window x:Class="WithoutName.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WithoutName"
        xmlns:language="clr-namespace:WithoutName.Properties.Localization"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <ComboBox Name="leng" SelectionChanged="ComboBox_SelectionChanged">
            <ComboBoxItem> English</ComboBoxItem>
            <ComboBoxItem> Russian</ComboBoxItem>
            <ComboBoxItem> Ukranian</ComboBoxItem>
        </ComboBox>
        <Label Content="{x:Static language:Lang.test}"></Label>
        
    </StackPanel>
</Window>
